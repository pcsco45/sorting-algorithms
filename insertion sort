numbers = [1, 7, 4, 9, 6]

for index in range (1, len(numbers)):
    current = numbers[index]
    counter = index
    while (counter > 0) and (numbers[counter - 1] > current):
        numbers[counter] = numbers[counter - 1]
        counter = counter - 1
    numbers[counter] = current

print(numbers)
